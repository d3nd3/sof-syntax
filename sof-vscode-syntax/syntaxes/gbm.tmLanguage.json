{
  "scopeName": "source.gbm",
  "name": "SoF - Ghoul Bolton Model",
  "patterns": [
    { "include": "#strings" },
    { "include": "#numbers" },
    { "include": "#keywords" }
  ],
  "repository": {
    "strings": {
      "patterns": [
        {
          "name": "string.gbm",
          "match": "(.+$\\n)|(.+)"
        }
      ]
    },
    "numbers": {
      "patterns": [
        {
          "name": "constant.numeric.gbm",
          "match": "[0-9]+"
        }
      ]
    },
    "keywords": {
      "patterns": [
        {
          "name": "entity.name.gbm",
          "match": ".+$\\n"
        },
        {
          "name": "variable.function.gbm",
          "match": ".+$\\n"
        },
        {
          "name": "keyword.gbm",
          "match": "(^[\\s]*\\n)|(.)"
        },
        {
          "name": "keyword.gbm",
          "match": "((\\w+\\s)(\\w+\\s))"
        },
        {
          "name": "keyword.gbm",
          "match": "(\\{\\})"
        }
      ]
    }
  }
}