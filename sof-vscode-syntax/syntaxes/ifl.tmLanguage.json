{
  "scopeName": "source.ifl",
  "name": "SoF - Image Files",
  "patterns": [
    { "include": "#numbers" },
    { "include": "#keywords" }
  ],
  "repository": {
    "numbers": {
      "patterns": [
        {
          "name": "constant.numeric.ifl",
          "match": "\\d"
        }
      ]
    },
    "keywords": {
      "patterns": [
        {
          "name": "entity.name.ifl",
          "match": "(\\.[tT][gG][aA])$\\n?"
        }
      ]
    }
  }
}