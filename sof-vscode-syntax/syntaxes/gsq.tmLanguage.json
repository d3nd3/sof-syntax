{
  "scopeName": "source.gsq",
  "name": "SoF - Ghoul Sequences",
  "patterns": [
    { "include": "#keywords" },
    { "include": "#numbers" },
    { "include": "#strings" }
  ],
  "repository": {
    "keywords": {
      "patterns": [
        {
          "name": "keyword.gsq",
          "match": "\\#"
        },
        {
          "name": "entity.name.gsq",
          "match": ".+$\\n"
        }
      ]
    },
    "numbers": {
      "patterns": [
        {
          "name": "constant.numeric.gsq",
          "match": "\\d"
        }
      ]
    },
    "strings": {
      "patterns": [
        {
          "name": "string.gsq",
          "match": "\\_"
        }
      ]
    }
  }
}